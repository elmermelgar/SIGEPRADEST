{% extends 'base2.html.twig' %}
{% block active %}active-menu{% endblock %}
{% block styles %}
    <link href="{{ asset('public/assets/js/dataTables/dataTables.bootstrap.css') }}" rel="stylesheet"/>
{% endblock %}
{% block Header %}
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li><a href="{{ path('alumno_expediente') }}">Inicio</a></li>
            <li class="active">Expedientes</li>
        </ol>
        <h2>Expedientes </h2>
    </div>
{% endblock %}
{% block body %}
<div class="col-md-12">
    <!--   Kitchen Sink -->
    <div class="panel panel-default">
        <div class="panel-heading">
            Expedientes en el sistema
        </div>

        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                    <tr>

                <th>Nombre</th>
                {#<th>Datoscarrera</th>#}
                {#<th>Genero</th>#}
                {#<th>País de nacimiento</th>#}
                {#<th>Nacionalidad</th>#}
                {#<th>Fecha nacimiento</th>#}
                <th>Dui</th>
                {#<th>Nit</th>#}
                {#<th>Estado civil </th>#}
                {#<th>Situación laboral</th>#}
                {#<th>Tipo de estudiante</th>#}
                <th>Direccion</th>
                {#<th>Departamento de residencia</th>#}
                {#<th>Municipio de residencia</th>#}
                <th>Teléfono</th>
                <th>Correo Electrónico</th>
                {#<th>Fecha titulación</th>#}
                {#<th>Fecha de titulación</th>#}
                {#<th>Institución</th>#}
                {#<th>Bachillerato</th>#}
                {#<th>Cambios de carrera aprobado</th>#}
                {#<th>Fecha registro</th>#}
                <th>Acciones </th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                {#<td><a href="{{ path('alumno_expediente_show', { 'id': entity.idExp }) }}">{{ entity.idExp }}</a></td>#}
                <td>{{ entity.nombreExp }}</td>
                {#<td>{{ entity.datosCarrera }}</td>#}
                {#<td>{{ entity.genero }}</td>#}
                {#<td>{{ entity.paisNacimiento }}</td>#}
                {#<td>{{ entity.nacionalidad }}</td>#}
                {#<td>{% if entity.fechaNaci %}{{ entity.fechaNaci|date('Y-m-d') }}{% endif %}</td>#}
                <td>{{ entity.duiExp }}</td>
                {#<td>{{ entity.nitExp }}</td>#}
                {#<td>{{ entity.estadoCivilExp }}</td>#}
                {#<td>{{ entity.situacionLaboral }}</td>#}
                {#<td>{{ entity.tipoEstudiante }}</td>#}
                <td>{{ entity.direccionExp }}</td>
                {#<td>{{ entity.deptoRecidencia }}</td>#}
                {#<td>{{ entity.municipioRecidencia }}</td>#}
                <td>{{ entity.telefono }}</td>
                <td>{{ entity.emailExp }}</td>
                {#<td>{% if entity.fechaTitulacion %}{{ entity.fechaTitulacion|date('Y-m-d H:i:s') }}{% endif %}</td>#}
                {#<td>{% if entity.fechaExpTitulo %}{{ entity.fechaExpTitulo|date('Y-m-d H:i:s') }}{% endif %}</td>#}
                {#<td>{{ entity.institucionExp }}</td>#}
                {#<td>{{ entity.bachillerato }}</td>#}
                {#<td>{{ entity.cambiosCarreraAprob }}</td>#}
                {#<td>{% if entity.fechaRegistro %}{{ entity.fechaRegistro|date('Y-m-d') }}{% endif %}</td>#}
                <td>
                <ul>
                    <a href="{{ path('alumno_expediente_show', { 'id': entity.idExp }) }}" title="Detalles" class="btn btn-default" style="background: #81379C;color: #ffffff;"><span class="glyphicon glyphicon-eye-open" aria-hidden="true"/></a>
                    -
                    <a href="{{ path('alumno_expediente_edit', { 'id': entity.idExp }) }}" title="Editar" class="btn btn-default" style="background: #0AA5E2;color: #ffffff;"><span class="glyphicon glyphicon-pencil" aria-hidden="true"/></a>




                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
    {#<a href="{{ path('alumno_new') }}"> Create a new entry </a>#}

{% endblock %}
{% block javascripts %}
    <script src="{{ asset('public/assets/js/dataTables/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('public/assets/js/dataTables/dataTables.bootstrap.js') }}"></script>
    <script>
        $(document).ready(function () {
            $('#dataTables-example').dataTable();
        });
    </script>
{% endblock %}