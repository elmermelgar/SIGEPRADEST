<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cita" table="cita">
    <indexes>
      <index name="b_fk" columns="id_dhe"/>
      <index name="c_fk" columns="id_ui"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="cita_pk" columns="id_cita"/>
    </unique-constraints>
    <id name="idCita" type="integer" column="id_cita">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="comentarioCita" type="string" column="comentario_cita" length="200" nullable="true"/>
    <many-to-one field="idUi" target-entity="Usuario">
      <join-columns>
        <join-column name="id_ui" referenced-column-name="id_ui"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDhe" target-entity="DetalleHorario">
      <join-columns>
        <join-column name="id_dhe" referenced-column-name="id_dhe"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
